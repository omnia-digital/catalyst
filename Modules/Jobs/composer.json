{
  "name": "omnia-digital/jobs",
  "description": "",
  "authors": [
    {
      "name": "Joshua Torres",
      "email": "josht@omniadigital.io"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "blade-ui-kit/blade-heroicons": "^0.3.1",
    "brunocfalcao/blade-feather-icons": "^1.0",
    "calebporzio/sushi": "^2.1",
    "doctrine/dbal": "^2.12",
    "epartment/nova-dependency-container": "^1.2",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "google/apiclient": "^2.9",
    "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
    "guzzlehttp/guzzle": "^7.3",
    "kabbouchi/nova-impersonate": "^1.3",
    "laravel/cashier": "^12.6",
    "laravel/framework": "^8.12",
    "laravel/horizon": "^5.5",
    "laravel/jetstream": "^1.6",
    "laravel/nova": "~3.0",
    "laravel/sanctum": "^2.6",
    "laravel/slack-notification-channel": "^2.3",
    "laravel/tinker": "^2.5",
    "league/iso3166": "^3.0.0",
    "livewire/livewire": "^2.0",
    "optimistdigital/nova-settings": "^2.6",
    "sentry/sentry-laravel": "^2.5",
    "sixlive/nova-text-copy-field": "^1.5",
    "spatie/data-transfer-object": "^2.6",
    "spatie/laravel-permission": "^3.17",
    "spatie/laravel-sluggable": "^3.0",
    "spatie/laravel-validation-rules": "^3.0",
    "thujohn/twitter": "^2.3",
    "vyuldashev/nova-permission": "^2.9"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [],
      "aliases": {
      }
    }
  },
  "autoload": {
    "psr-4": {
      "Modules\\Jobs\\": ""
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "@php artisan nova:publish",
      "@php artisan horizon:publish --ansi"
    ],
    "pre-autoload-dump": "Google\\Task\\Composer::cleanup"
  }
}
