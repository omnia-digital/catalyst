{
    "name": "omnia-digital/catalyst",
    "type": "project",
    "description": "The Community Platform Framework",
    "keywords": [
        "framework",
        "catalyst",
        "community",
        "community platform framework",
        "livewire",
        "laravel",
        "filament"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-zlib": "*",
        "ariaieboy/filament-jalali-datetime": "^1.0",
        "awcodes/filament-badgeable-column": "^2.0",
        "awssat/laravel-visits": "^6.0",
        "bezhansalleh/filament-exceptions": "^2.0",
        "bezhansalleh/filament-google-analytics": "^2.0.1",
        "bezhansalleh/filament-shield": "^3.0",
        "blade-ui-kit/blade-heroicons": "^2.1",
        "calebporzio/sushi": "^2.4",
        "codeat3/blade-teeny-icons": "^1.9",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^3.0-stable",
        "filament/forms": "^3.0-stable",
        "filament/spatie-laravel-media-library-plugin": "^3.0-stable",
        "filament/spatie-laravel-settings-plugin": "^3.0-stable",
        "filament/spatie-laravel-tags-plugin": "^3.0-stable",
        "filament/spatie-laravel-translatable-plugin": "^3.0-stable",
        "filament/tables": "^3.0-stable",
        "fruitcake/php-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^10.0",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/cashier": "^14.12",
        "laravel/fortify": "^1.18",
        "laravel/framework": "^10.10",
        "laravel/horizon": "^5.15",
        "laravel/jetstream": "^4.0",
        "laravel/passport": "^11.9",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "laravel/vapor-cli": "^1.54",
        "laravel/vapor-core": "^2.28",
        "laravel/vapor-ui": "^1.7",
        "league/commonmark": "^2.3",
        "liran-co/laravel-notification-subscriptions": "^1.7",
        "livewire/livewire": "^3.0",
        "marcreichel/igdb-laravel": "^3.6",
        "mhmiton/laravel-modules-livewire": "^2",
        "nwidart/laravel-modules": "^9.0",
        "omnia-digital/catalyst-core-plugin": "*",
        "omnia-digital/catalyst-social-plugin": "*",
        "omnia-digital/library": "dev-main",
        "omnia-digital/livewire-calendar": "^3.0",
        "omnia-digital/media-manager": "dev-main",
        "omniphx/forrest": "*",
        "overtrue/laravel-follow": "*",
        "owenvoke/blade-fontawesome": "^2.2",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpsa/filament-password-reveal": "^2.0",
        "pusher/pusher-php-server": "^7.0",
        "ralphjsmit/laravel-filament-components": "^1.1",
        "sentry/sentry-laravel": "^3.0",
        "shuvroroy/filament-spatie-laravel-backup": "*",
        "shuvroroy/filament-spatie-laravel-health": "^2.0",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-analytics": "^5.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-data": "^3.4",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-searchable": "^1.11",
        "spatie/laravel-settings": "^2.4",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/laravel-tags": "^4.3",
        "squirephp/countries-en": "^3.3",
        "stechstudio/filament-impersonate": "^3.5",
        "stripe/stripe-php": "^9.0",
        "thomasjohnkane/snooze": "^2.2",
        "tightenco/ziggy": "^1.4",
        "vedmant/laravel-feed-reader": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^3.0-stable",
        "laracademy/generators": "^3.4",
        "laravel-lang/lang": "^13",
        "laravel-lang/publisher": "^14.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/pint": "^1.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0",
        "tightenco/duster": "^1.1"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        },
        {
            "type": "path",
            "url": "../modules/catalyst-core-plugin"
        },
        {
            "type": "path",
            "url": "../modules/catalyst-social-plugin"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/",
            "Themes\\": "resources/views/themes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan vendor:publish --tag=library-assets --force",
            "@php artisan vapor-ui:publish --ansi",
            "@php artisan filament:upgrade",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan vendor:publish --tag mailcoach-assets --force",
            "@php artisan icons:cache"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "joshbrw/laravel-module-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
