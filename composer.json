{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awssat/laravel-visits": "^3.0",
        "blade-ui-kit/blade-heroicons": "^1.3",
        "calebporzio/sushi": "^2.4",
        "doctrine/dbal": "^3.3",
        "filament/filament": "^2.0",
        "filament/forms": "^2.0",
        "filament/tables": "^2.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jorenvanhocht/laravel-share": "^4.2",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.4",
        "laravel/nova": "^3.9",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravel/vapor-cli": "^1.38",
        "laravel/vapor-core": "^2.21",
        "laravel/vapor-ui": "^1.5",
        "livewire/livewire": "^2.5",
        "mhmiton/laravel-modules-livewire": "^1.3",
        "nwidart/laravel-modules": "^8.2",
        "omnia-digital/library": "dev-main",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phuclh/media-manager": "^0.0.2",
        "pusher/pusher-php-server": "^7.0",
        "sentry/sentry-laravel": "^2.11",
        "spatie/laravel-activitylog": "^4.4",
        "spatie/laravel-medialibrary": "^10.0.0",
        "spatie/laravel-sluggable": "^3.1",
        "spatie/laravel-tags": "^4.3",
        "squirephp/countries-en": "^3.3",
        "tightenco/ziggy": "^1.4",
        "wimil/followers": "^1.0"
    },
    "require-dev": {
        "arcanedev/laravel-lang": "^10.0",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "laracademy/generators": "^3.4",
        "laravel-lang/lang": "^10.9",
        "laravel-lang/publisher": "^13.0",
        "laravel/sail": "^1.12",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../omnia-library"
        },
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi",
            "@php artisan vendor:publish --tag=library-assets --force",
            "@php artisan filament:upgrade",
            "@php artisan config:clear",
            "@php artisan view:clear",
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
